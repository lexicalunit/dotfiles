"body .native-key-bindings":
  # I never want shift-tab to move focus from the text-editor to other elements.
  "shift-tab": "unset!"

"atom-workspace":
  # Removes `api-workbench:find-usages`; it conflicts with `go-to-line:toggle`
  "ctrl-g": "unset!"

"atom-text-editor":
  # Replaces `editor:select-line` with `atom-notes:toggle`
  "cmd-l": "atom-notes:toggle"

  # Use the same spell check shortcut as vscode
  "cmd-.": "spell-check:correct-misspelling"
