# .profile_check

check() {
    echo $'\e[34m                                  ..*////*.                               \e[0m'
    echo $'\e[34m                         ,@@@@@@@@@@@@@@@@@@@@@@@@@(.                     \e[0m'
    echo $'\e[34m                    /@@@@@@@@@(,. %@@*  @@@@@@@@@@@@@@@@#.                \e[0m'
    echo $'\e[34m                ,@@@@@@&.     ,(&@@@@*  @@@@@@@@@@@@@@@@@@@@/             \e[0m'
    echo $'\e[34m             .@@@@@&.   ,@@@@@@@@@@@@*             *#@@@@@@@@@@*          \e[0m'
    echo $'\e[34m           *@@@@&   .@@@@@@@@@@@@@@@@*  (.    ,@@@&*    .%@@@@@@@&        \e[0m'
    echo $'\e[34m         *@@@@(   @@@@@@@@@@@@@@%*(@@*  @@@@@%   .&@@@@@*   .@@@@@@&      \e[0m'
    echo $'\e[34m       .@@@@@. .@@@@@@@@@@@,    ,*@@@@@@@@@@@@@@@   *@@@@@@#.  *@@@@@/    \e[0m'
    echo $'\e[34m      (@@@@@@  .@@@@@@@@   ,@@@@@@@@@@@@@@@@@@@@@@@/  ,@@@@@@@(  .@@@@@   \e[0m'
    echo $'\e[34m     #@@@@@@@@   *@@&,  .@@@@@@(             ,@@@@@@@,  #@@@@@@@#,&@@@@@  \e[0m'
    echo $'\e[34m    (@@@/  @@@@@%.   ,&@@@@@.                    #@@@@@  ,@@@@@@@@@@&.    \e[0m'
    echo $'\e[34m   ,@@@%  #@@  .@@@@@@@@@@                         (@@@@#@@@@@@&.         \e[0m'
    echo $'\e[34m   &@@@. ,@@*  @@#  %@@@(                           .@@@@@%,              \e[0m'
    echo $'\e[34m   @@@%  %@@  (@@  *@@@%                             ,,                   \e[0m'
    echo $'\e[34m  *@@@/  @@%  @@&  %@@@.                                                  \e[0m'
    echo $'\e[34m  *@@@/  @@%  @@&  %@@@.                                                  \e[0m'
    echo $'\e[34m   @@@&  %@@  /@@  ,@@@&                             **                   \e[0m'
    echo $'\e[34m   &@@@. ,@@*  @@%  %@@@#                           ,@@@@@@*              \e[0m'
    echo $'\e[34m   ,@@@&  #@@  .@@#  #@@@@                         #@@@@@@@@@@@@*         \e[0m'
    echo $'\e[34m    (@@@/  &@@      .&@@@@@@.                    %@@@@@/   ./@@@@@@@@*    \e[0m'
    echo $'\e[34m     (@@@(      ,@@@@@%. @@@@@@(.            ,@@@@@@*  .&@@%.  (@@@@@@@@  \e[0m'
    echo $'\e[34m      /@@@@.#@@@@@&.   /@@@@@@@@@@@@@@@@@@@@@@@@@*   (@@@@@@@%  &@@@@@@   \e[0m'
    echo $'\e[34m        @@@@@@&.   .  .@@@@@@@@@@@@@@@@@@@@#,    .&@@@@@@@@@@/  @@@@@*    \e[0m'
    echo $'\e[34m         *@@@@(,@@@@@@.   /@@@@@@@@@@*  @@@*(@@@@@@@@@@@@@@*  .@@@@&      \e[0m'
    echo $'\e[34m           *@@@@@@@@@@@@@#     .*%@@@*  @@@@@@@@@@@@@@@@/   (@@@@%        \e[0m'
    echo $'\e[34m             .@@@@@@@@@@@@@@@@@%,.      @@@@@@@@@@@@/    #@@@@@/          \e[0m'
    echo $'\e[34m                .@@@@@@@@@@@@@@@@@@@@*  @@@@@#*.     (@@@@@@/             \e[0m'
    echo $'\e[34m                    /@@@@@@@@@@@@@@@@*  @@@, ./&@@@@@@@@#                 \e[0m'
    echo $'\e[34m                         *%@@@@@@@@@@@@@@@@@@@@@@@@/.                     \e[0m'
    echo
}

check1() {
    echo
    echo $'\e[34m                                                  \e[0m'
    echo $'\e[34m                         *#%%%%%%%%(.             \e[0m'
    echo $'\e[34m                   #%%%%%%%%%%%%%%%%%%%%%         \e[0m'
    echo $'\e[34m               #%%%%%%%%%%%%%%%%%%%%%%%%%%%#      \e[0m'
    echo $'\e[34m            (%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%     \e[0m'
    echo $'\e[34m          %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%    \e[0m'
    echo $'\e[34m        #%%%%%%%%%%%%/            %%%%%%%%%%%%#   \e[0m'
    echo $'\e[34m       %%%%%%%%%%%#                %%%%%%%%%%%%   \e[0m'
    echo $'\e[34m      %%%%%%%%%%%      #%%%%% (%%%%%%%%%%%%%%%(   \e[0m'
    echo $'\e[34m     %%%%%%%%%%%      %%%%%%%%%%%%%%%%%%%%%%%%    \e[0m'
    echo $'\e[34m    (%%%%%%%%%%%      %%%%%( ,%%%%%%%%%%%%%%%     \e[0m'
    echo $'\e[34m    %%%%%%%%%%%%                #%%%%%%%%%%%      \e[0m'
    echo $'\e[34m    (%%%%%%%%%%%%%           #%%%%%%%%%%%%,       \e[0m'
    echo $'\e[34m     %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%/         \e[0m'
    echo $'\e[34m      %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%            \e[0m'
    echo $'\e[34m        %%%%%%%%%%%%%%%%%%%%%%%%%%%               \e[0m'
    echo $'\e[34m          ,%%%%%%%%%%%%%%%%%%%#                   \e[0m'
    echo $'\e[34m                 *(##(*.                          \e[0m'
    echo $'\e[34m                                                  \e[0m'
    echo $'\e[34m                                                  \e[0m'
    echo
}
