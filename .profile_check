# .profile_check

check () {
	echo
	echo $'\e[34m                                                  \e[0m'
	echo $'\e[34m                         *#%%%%%%%%(.             \e[0m'
	echo $'\e[34m                   #%%%%%%%%%%%%%%%%%%%%%         \e[0m'
	echo $'\e[34m               #%%%%%%%%%%%%%%%%%%%%%%%%%%%#      \e[0m'
	echo $'\e[34m            (%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%     \e[0m'
	echo $'\e[34m          %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%    \e[0m'
	echo $'\e[34m        #%%%%%%%%%%%%/            %%%%%%%%%%%%#   \e[0m'
	echo $'\e[34m       %%%%%%%%%%%#                %%%%%%%%%%%%   \e[0m'
	echo $'\e[34m      %%%%%%%%%%%      #%%%%% (%%%%%%%%%%%%%%%(   \e[0m'
	echo $'\e[34m     %%%%%%%%%%%      %%%%%%%%%%%%%%%%%%%%%%%%    \e[0m'
	echo $'\e[34m    (%%%%%%%%%%%      %%%%%( ,%%%%%%%%%%%%%%%     \e[0m'
	echo $'\e[34m    %%%%%%%%%%%%                #%%%%%%%%%%%      \e[0m'
	echo $'\e[34m    (%%%%%%%%%%%%%           #%%%%%%%%%%%%,       \e[0m'
	echo $'\e[34m     %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%/         \e[0m'
	echo $'\e[34m      %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%            \e[0m'
	echo $'\e[34m        %%%%%%%%%%%%%%%%%%%%%%%%%%%               \e[0m'
	echo $'\e[34m          ,%%%%%%%%%%%%%%%%%%%#                   \e[0m'
	echo $'\e[34m                 *(##(*.                          \e[0m'
	echo $'\e[34m                                                  \e[0m'
	echo $'\e[34m                                                  \e[0m'
	echo
}
