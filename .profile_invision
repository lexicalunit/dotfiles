# .profile_invision

studio() {
    cat <<EOF

╭━━━╮╭╮╱╱╱╱╱╭╮
┃╭━╮┣╯╰╮╱╱╱╱┃┃
┃╰━━╋╮╭╋╮╭┳━╯┣┳━━╮
╰━━╮┃┃┃┃┃┃┃╭╮┣┫╭╮┃
┃╰━╯┃┃╰┫╰╯┃╰╯┃┃╰╯┣╮
╰━━━╯╰━┻━━┻━━┻┻━━┻╯

EOF
}

invision() {
    echo $'\e[38;5;203m\e[48;5;21m''                                                                       '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''                                                                       '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''                                                              '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''              .%@@@/                                          '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''             &@@@@@@@                                         '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''            .@@@@@@@@,                                        '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''             /@@@@@@(                                         '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''                ,,.                                           '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''                                                              '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''        &&&&&&&&&&&&   &&&&&&&&&&&    (@@@@@%.                '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''       #@@@@@@@@@@@#  (@@@@@@@@@@/ (@@@@@@@@@@@(              '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''       @@@@@@@@@@@@   @@@@@@@@@@@*@@@@@@@@@@@@@@/             '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''           (@@@@@@#       *@@@@@@@@,    ,@@@@@@@@             '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''           @@@@@@@        @@@@@@@(       ,@@@@@@@             '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''          #@@@@@@/       (@@@@@@#        *@@@@@@#             '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''          @@@@@@@        @@@@@@@         @@@@@@@.             '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''         #@@@@@@/       (@@@@@@#        /@@@@@@#              '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''         @@@@@@@        @@@@@@@         @@@@@@@               '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''        #@@@@@@,       &@@@@@@/        %@@@@@@*   ..          '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''        @@@@@@@      ,@@@@@@@@         @@@@@@@    @@@.        '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''        @@@@@@@,  .(@@@@@@@@@/         @@@@@@@  *@@@*         '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''        (@@@@@@@@@@@@.@@@@@@@          #@@@@@@@@@@@(          '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''          %@@@@@@@#  #@@@@@@*            %@@@@@@@*            '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''    '$'\e[48;5;231m''                                                              '$'\e[48;5;21m''     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''                                                                       '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''                   Design better products, faster.                     '$'\e[0m'
    echo $'\e[38;5;203m\e[48;5;21m''                                                                       '$'\e[0m'
}

