{
  "editor.cursorBlinking": "phase",
  "editor.fontFamily": "IBMPlexMono, Inconsolata",
  "editor.fontSize": 18,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "boundary",
  "editor.wordWrapColumn": 120,
  "go.formatTool": "goimports",
  "go.lintTool": "gometalinter",
  "go.toolsGopath": "/usr/local/lib/gopath:/usr/local/lib/invision/gopath",
  "window.nativeTabs": false,
  "workbench.colorTheme": "Solarized Light",
  "workbench.editor.closeOnFileDelete": false,
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#057eff",
    "[Solarized Light]": {
      "editor.selectionHighlightBackground": "#eee8d5",
      "editor.selectionHighlightBorder": "#76736a",
      "editor.lineHighlightBackground": "#f9f2df"
    }
  },
  "editor.rulers": [72, 80, 100, 120],
  "workbench.statusBar.feedback.visible": false,
  "shellformat.flag": "-s -i 4",
  "files.trimTrailingWhitespace": true,
  "editor.codeLens": false,
  "files.insertFinalNewline": true,
  "explorer.openEditors.visible": 0,
  "editor.wordWrap": "bounded",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderIndentGuides": false,
  "workbench.editor.showTabs": true,
  "[css]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "[shellscript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 4
  },
  "workbench.iconTheme": "vscode-icons",
  "explorer.autoReveal": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "markup.italic"
        ],
        "settings": {
          // "fontStyle": ""
        }
      }
    ]
  },
  "tsimporter.preferRelative": false,
  "tsimporter.showNotifications": true,
  "importSorter.importStringConfiguration.trailingComma": "multiLine",
  "importSorter.importStringConfiguration.tabSize": 2,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "workbench.settings.editor": "json",
  "terminal.integrated.fontFamily": "Hack Nerd Font",
}
