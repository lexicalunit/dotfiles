{
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "phase",
  "editor.fontFamily": "Fira Code, Inconsolata",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "boundary",
  "editor.wordWrapColumn": 120,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll": "explicit"
  },
  "window.nativeTabs": false,
  "workbench.colorTheme": "Solarized Light",
  "workbench.editor.closeOnFileDelete": false,
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#057eff",
    "[Solarized Light]": {
      "editor.selectionHighlightBackground": "#eee8d5",
      "editor.selectionHighlightBorder": "#76736a",
      "editor.lineHighlightBackground": "#f9f2df"
    }
  },
  "editor.rulers": [72, 80, 100, 120],
  "editor.guides.bracketPairs": true,
  "shellformat.flag": "-s -i 4",
  "files.trimTrailingWhitespace": true,
  "editor.codeLens": false,
  "files.insertFinalNewline": true,
  "git.mergeEditor": true,
  "explorer.openEditors.visible": 0,
  "editor.wordWrap": "off",
  "editor.multiCursorModifier": "alt",
  "workbench.editor.showTabs": "multiple",
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "[css]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[shellscript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 4
  },
  "[python]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.rulers": [88, 100],
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never"
    }
  },
  "files.associations": {
    ".gitconfig": "ini"
  },
  "[ini]": {
    "editor.formatOnSave": false
  },
  "python.formatting.provider": "none",
  "explorer.autoReveal": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "markup.italic"
        ],
        "settings": {
          // "fontStyle": ""
        }
      }
    ]
  },
  "importSorter.importStringConfiguration.trailingComma": "multiLine",
  "importSorter.importStringConfiguration.tabSize": 2,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "javascript.validate.enable": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.formatOnSave": true,
  "window.restoreWindows": "none",
  "window.openFoldersInNewWindow": "off",
  "workbench.settings.useSplitJSON": true,
  "workbench.settings.editor": "json",
  "telemetry.telemetryLevel": "off",
  "window.openWithoutArgumentsInNewWindow": "off",
  "window.closeWhenEmpty": true,
  "python.linting.flake8Enabled": false,
  "python.linting.pylintEnabled": false,
  "python.languageServer": "Pylance",
  "python.analysis.typeCheckingMode": "basic",
  "extensions.ignoreRecommendations": true,
  "gitlens.currentLine.pullRequests.enabled": false,
  "shellcheck.customArgs": ["-x"],
  "terminal.integrated.inheritEnv": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.editor.restoreViewState": false,
  "window.newWindowDimensions": "inherit",
  "files.hotExit": "off",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "security.workspace.trust.banner": "always",
  "security.workspace.trust.enabled": false,
  "redhat.telemetry.enabled": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
