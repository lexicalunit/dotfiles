{
  "editor.cursorBlinking": "phase",
  "editor.fontFamily": "Fira Code, Inconsolata",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "boundary",
  "editor.wordWrapColumn": 120,
  "go.formatTool": "goimports",
  "go.lintTool": "golint",
  "go.toolsGopath": "/usr/local/lib/gopath:/usr/local/lib/invision/gopath",
  "window.nativeTabs": false,
  "workbench.colorTheme": "Solarized Light",
  "workbench.editor.closeOnFileDelete": false,
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#057eff",
    "[Solarized Light]": {
      "editor.selectionHighlightBackground": "#eee8d5",
      "editor.selectionHighlightBorder": "#76736a",
      "editor.lineHighlightBackground": "#f9f2df"
    }
  },
  "editor.rulers": [
    72,
    80,
    100,
    120
  ],
  "shellformat.flag": "-s -i 4",
  "files.trimTrailingWhitespace": true,
  "editor.codeLens": false,
  "files.insertFinalNewline": true,
  "explorer.openEditors.visible": 0,
  "editor.wordWrap": "off",
  "editor.multiCursorModifier": "alt",
  "editor.renderIndentGuides": false,
  "workbench.editor.showTabs": true,
  "[css]": {
    "editor.tabSize": 2
  },
  "[ruby]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "[shellscript]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 4
  },
  "[python]": {
    "editor.rulers": [
        90
    ],
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "python.formatting.provider": "black",
  "explorer.autoReveal": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "markup.italic"
        ],
        "settings": {
          // "fontStyle": ""
        }
      }
    ]
  },
  "importSorter.importStringConfiguration.trailingComma": "multiLine",
  "importSorter.importStringConfiguration.tabSize": 2,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "javascript.validate.enable": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.formatOnSave": false,
  "spellright.language": [
    "en"
  ],
  "spellright.documentTypes": [
    "markdown",
    "latex",
    "plaintext",
    "shellscript"
  ],
  "window.restoreWindows": "none",
  "window.openFoldersInNewWindow": "off",
  "workbench.settings.useSplitJSON": true,
  "workbench.settings.editor": "json",
  "telemetry.enableTelemetry": false,
  "gitlens.advanced.telemetry.enabled": false,
  "window.openWithoutArgumentsInNewWindow": "off",
  "window.closeWhenEmpty": true,
  "spellright.spellContext": "body comments strings",
  "spellright.spellContextByClass": {
    "shellscript": "body comments",
  },
  "spellright.notificationClassByParser": {
    "code": "information"
  },
  "ruby.rubocop.useBundler": true,
  "ruby.rubocop.suppressRubocopWarnings": true,
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": false,
  "python.languageServer": "Pylance",
  "python.analysis.typeCheckingMode": "basic",
  "window.zoomLevel": 0,
  "python.showStartPage": false,
  "extensions.ignoreRecommendations": true,
  "telemetry.enableCrashReporter": false,
  "gitlens.currentLine.pullRequests.enabled": false
}
