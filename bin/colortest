for($dimone = 0; $dimone < scalar @arr; $dimone++)
{
    $seed = ($dimone % 6) * -1;
    for($dimtwo = 0; $dimtwo < 6; $dimtwo++)
    {
        $movone = $seed;
        $movtwo = $seed * -1;
        $newone = $dimone+$movone;
        $newtwo = $dimtwo+$movtwo;
        if($newone < scalar @arr)
        {
            print $arr[$newone][$newtwo] . "   ";
        }
        $seed = $seed+1;
    }
    print "\n";
}
print "\n";
print "\n";
